{
    "name": "idecardo/laravel-ui-installer",
    "description": "Laravel UI Installer.",
    "keywords": ["laravel", "ui", "installer"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Rey De Cardo",
            "email": "ivanreydecardo@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.9",
        "symfony/console": "^5.0",
        "symfony/filesystem": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "bin": [
        "bin/laravel-ui-installer"
    ],
    "autoload": {
        "psr-4": {
            "Idecardo\\LaravelUIInstaller\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Idecardo\\LaravelUIInstaller\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
